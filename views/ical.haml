BEGIN:VCALENDAR
VERSION:2.0
CALSCALE:GREGORIAN
PRODID:-//feedine//NONSGML//
BEGIN:VTIMEZONE
TZID:Asia/Tokyo
BEGIN:STANDARD
TZOFFSETFROM:+0900
TZOFFSETTO:+0900
DTSTART:19700101T000000
TZNAME:JST
END:STANDARD
END:VTIMEZONE
- feed[:entries].each do |entry|
  BEGIN:VEVENT
  = "URL:#{entry[:link]}"
  = "CATEGORIES:#{entry[:title]}"
  = "DESCRIPTION:#{entry[:description]} #{entry[:link]}"
  = "DTSTAMP:#{ical_at(entry[:date])}T000000"
  = "DTSTART;TZID=Asia/Tokyo:#{ical_at(entry[:date])}"
  = "DTEND;TZID=Asia/Tokyo:#{ical_at(entry[:date])}"
  - if entry[:lat] and entry[:lng]
    = "LOCATION:#{entry[:lat]},#{entry[:lng]}"
  = "SUMMARY:#{entry[:title]}"
  = "UID:#{entry[:title]}-#{entry[:link]}"
  END:VEVENT
END:VCALENDAR
